{
  "name": "vue3-monorepo-template",
  "version": "1.0.0",
  "description": "Vue 3 Monorepo Template with TypeScript, Vite, and Performance Optimizations",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "pnpm --filter @template/sample-desktop dev",
    "dev:desktop": "pnpm --filter @template/desktop dev",
    "dev:sample-desktop": "pnpm --filter @template/sample-desktop dev",
    "dev:mobile": "pnpm --filter @template/mobile dev",
    "build": "tsx shared/scripts/build-order.ts",
    "build:parallel": "pnpm -r run build",
    "build:packages": "pnpm --filter \"./packages/**\" build",
    "build:apps": "pnpm --filter \"./apps/**\" build",
    "build:sample-desktop": "pnpm --filter @template/sample-desktop build",
    "build:desktop": "pnpm --filter @template/desktop build",
    "build:mobile": "pnpm --filter @template/mobile build",
    "build:analyze:desktop": "pnpm --filter @template/desktop build:analyze",
    "build:analyze:mobile": "pnpm --filter @template/mobile build:analyze",
    "build:analyze:sample-desktop": "pnpm --filter @template/sample-desktop build:analyze",
    "test": "pnpm -r run test",
    "test:utils": "npx vitest run packages/utils/src/__tests__/",
    "test:ui": "npx vitest run packages/ui/src/__tests__/",
    "test:api": "npx vitest run packages/api/src/__tests__/",
    "test:watch": "npx vitest",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "lint:check": "eslint . --ext .vue,.js,.ts,.jsx,.tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "pnpm -r run type-check",
    "clean": "pnpm -r run clean",
    "clean:all": "pnpm -r run clean && rm -rf node_modules pnpm-lock.yaml",
    "deps:check": "pnpm list --depth=10",
    "deps:circular": "npx madge --circular packages/",
    "deps:tree": "pnpm list --depth=5 --json",
    "storybook": "pnpm --filter @template/ui storybook",
    "storybook:external": "pnpm --filter @template/ui storybook:external",
    "build-storybook": "pnpm --filter @template/ui build-storybook",
    "optimize-icons": "tsx shared/scripts/optimize-icons.ts",
    "mobile:start": "pnpm --filter @template/mobile-native start",
    "mobile:android": "pnpm --filter @template/mobile-native android",
    "mobile:ios": "pnpm --filter @template/mobile-native ios",
    "mobile:build": "pnpm --filter @template/mobile-native eas:build",
    "mobile:build:android": "pnpm --filter @template/mobile-native eas:build:android",
    "mobile:build:ios": "pnpm --filter @template/mobile-native eas:build:ios",
    "mobile:submit": "pnpm --filter @template/mobile-native eas:submit"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@vee-validate/zod": "^4.15.1",
    "date-fns": "^3.0.0",
    "date-fns-tz": "^3.2.0",
    "jsonc-parser": "^3.3.1",
    "kebab-case": "^2.0.2",
    "pinia": "^2.1.0",
    "vee-validate": "^4.15.1",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@locator/runtime": "^0.4.4",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-themes": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/vue3": "^8.6.14",
    "@storybook/vue3-vite": "^8.6.14",
    "@tokens-studio/sd-transforms": "^2.0.1",
    "@types/node": "^20.19.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.2.0",
    "globals": "^16.2.0",
    "jsdom": "^24.0.0",
    "madge": "^8.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "prettier-plugin-sort-imports": "^1.8.8",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rimraf": "^5.0.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.69.5",
    "sd-tailwindcss-transformer": "^2.1.0",
    "storybook": "^8.6.14",
    "style-dictionary": "^4.4.0",
    "svgo": "^4.0.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^5.4.19",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-inspect": "^11.3.0",
    "vue-tsc": "^1.8.0"
  },
  "msw": {
    "workerDirectory": [
      "apps/sample-desktop/public"
    ]
  }
}