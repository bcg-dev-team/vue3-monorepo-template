{
  "name": "@template/utils",
  "version": "1.0.0",
  "description": "공통 유틸리티 함수 패키지",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --noEmit && tsc -p tsconfig.build.json",
    "dev": "vite build --watch",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@template/types": "workspace:*",
    "zxcvbn": "^4.4.2"
  },
  "peerDependencies": {
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/zxcvbn": "^4.4.5"
  }
}
