{
  "name": "vue3-monorepo-template",
  "version": "1.0.0",
  "description": "Vue 3 Monorepo Template with TypeScript, Vite, and Performance Optimizations",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @template/desktop dev",
    "dev:desktop": "pnpm --filter @template/desktop dev",
    "dev:mobile": "pnpm --filter @template/mobile dev",
    "build": "tsx shared/scripts/build-order.ts",
    "build:parallel": "pnpm -r run build",
    "build:packages": "pnpm --filter \"./packages/**\" build",
    "build:apps": "pnpm --filter \"./apps/**\" build",
    "test": "pnpm -r run test",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "lint:check": "eslint . --ext .vue,.js,.ts,.jsx,.tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "pnpm -r run type-check",
    "clean": "pnpm -r run clean",
    "clean:all": "pnpm -r run clean && rm -rf node_modules pnpm-lock.yaml",
    "deps:check": "pnpm list --depth=10",
    "deps:circular": "npx madge --circular packages/",
    "deps:tree": "pnpm list --depth=5 --json",
    "storybook": "pnpm --filter @template/ui storybook",
    "build-storybook": "pnpm --filter @template/ui build-storybook",
    "mobile:start": "pnpm --filter @template/mobile-native start",
    "mobile:android": "pnpm --filter @template/mobile-native android",
    "mobile:ios": "pnpm --filter @template/mobile-native ios",
    "mobile:build": "pnpm --filter @template/mobile-native eas:build",
    "mobile:build:android": "pnpm --filter @template/mobile-native eas:build:android",
    "mobile:build:ios": "pnpm --filter @template/mobile-native eas:build:ios",
    "mobile:submit": "pnpm --filter @template/mobile-native eas:submit"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
"@locator/runtime": "^0.4.4",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.2.0",
    "globals": "^16.2.0",
    "prettier": "^3.0.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.35.1",
    "vite-plugin-inspect": "^11.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@vee-validate/zod": "^4.15.1",
    "vee-validate": "^4.15.1",
    "zod": "^3.25.67"
  }
}
